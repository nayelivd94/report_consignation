<openerp>
    <data>
        <record id="view_company_form_inherit" model="ir.ui.view">
            <field name="name">view.company.form.add.text.field</field>
            <field name="model">res.company</field>
            <field name="inherit_id" ref="base.view_company_form"/>
            <field name="arch" type="xml">
                <xpath expr="//notebook/page" position="after">
                    <page string="Consignación">
                            <group>
                                <group string="Configuración de Duplicidad">
                                    <field name="locationconsignation_id"/>
                                    <field name="suplier_id"/>
                                    <field name="companyconsignation_id"/>
                                </group>

                            </group>
                        </page>
                </xpath>
            </field>
    </record>
    </data>
</openerp>